             xmlns:models ="clr-namespace:Dimmer.Data.Models;assembly=Dimmer"
             Title="HomePage">

    <Grid x:Name="MainPage">
        <CollectionView ItemsSource="{Binding DisplayedSongs}" />
    </Grid>
    
</ContentPage>